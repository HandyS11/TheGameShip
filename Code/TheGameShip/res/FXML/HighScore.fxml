<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.HighScore">
   <children>
      <FlowPane prefHeight="720.0" prefWidth="1280.0" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="300.0" AnchorPane.rightAnchor="300.0" AnchorPane.topAnchor="50.0">
         <children>
            <GridPane prefHeight="620.0" prefWidth="680.0">
              <columnConstraints>
                <ColumnConstraints percentWidth="50" />
                  <ColumnConstraints percentWidth="50" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints percentHeight="10" />
                <RowConstraints percentHeight="70" />
                <RowConstraints percentHeight="20" />
              </rowConstraints>
               <children>
                   <Button fx:id="ButtonClear" mnemonicParsing="false" text="Clear" GridPane.halignment="CENTER" GridPane.rowIndex="2">
                     <font>
                        <Font size="20.0" />
                     </font></Button>
                   <Button fx:id="ButtonLeave" mnemonicParsing="false" text="Leave" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2">
                     <font>
                        <Font size="20.0" />
                     </font></Button>
                  <Label text="High Score" GridPane.columnSpan="2" GridPane.halignment="CENTER">
                     <font>
                        <Font size="25.0" />
                     </font>
                  </Label>
               </children>
            </GridPane>
         </children>
      </FlowPane>
   </children>
</AnchorPane>
