<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.control.ListView?>

<AnchorPane id="background" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.HighScoreView">
    <FlowPane prefHeight="720.0" prefWidth="1280.0" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="300.0" AnchorPane.rightAnchor="300.0" AnchorPane.topAnchor="50.0">
        <GridPane prefHeight="620.0" prefWidth="680.0">
            <columnConstraints>
                <ColumnConstraints percentWidth="50" />
                <ColumnConstraints percentWidth="50" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints percentHeight="10" />
                <RowConstraints percentHeight="70" />
                <RowConstraints percentHeight="20" />
            </rowConstraints>

            <Label id="label" text="High Score" GridPane.columnSpan="2" GridPane.halignment="CENTER" textFill="white" />
            <Button id="button" onAction="#menu" text="Leave"  GridPane.halignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Button id="button" onAction="#clear" text="Clear" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
            <ListView fx:id="listview" GridPane.columnSpan="2" GridPane.rowIndex="1" />
        </GridPane>
    </FlowPane>
</AnchorPane>

